    <!-- Top menu item -->

    <menuitem name="simarropop" id="simarropop.menu_root"/>

    <!-- menu categories -->
    <menuitem name="Simarropop Menu" id="simarropop.menu_1" parent="simarropop.menu_root"/>
     <!--<menuitem name="Menu 1" id="simarropop.menu_1" parent="simarropop.menu_root"/>
    -->
