<odoo>
  <data>
    <!-- explicit list view definition -->

<!-- Vista tree-->
    <record model="ir.ui.view" id="simarropop_valoraciones_tree">
      <field name="name">simarropop valoraciones tree</field>
      <field name="model">simarropop.valoracion</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          
        </tree>
      </field>
    </record>


<!-- Vista Form-->
    <record model="ir.ui.view" id="simarropop_valoraciones_form">
      <field name="name">simarropop valoraciones form</field>
      <field name="model">simarropop.valoracion</field>
      <field name="arch" type="xml">
        <form>
        <group>
          <field name="name"/>
          <field name="usuario" domain="[('is_user','=',True)]"/>
          <field name="opinion" />
          <field name="puntuacion"/>
          </group>
        </form>
      </field>
    </record>


    
  </data>
</odoo>